{
  "name": "nataniel/e4u-framework",
  "description": "E4u Framework",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Artur Jedlinski",
      "email": "artur@jedlinski.pl"
    }
  ],
  "config": {
    "platform": {
      "php": "8.4"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "php-http/discovery": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5"
  },
  "require": {
    "php": "^8.4",
    "ext-mbstring": "*",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-readline": "*",
    "ext-exif": "*",
    "ext-gd": "*",
    "ext-zip": "*",
    "doctrine/orm": "^3.5",
    "doctrine/migrations": "^3.9",
    "doctrine/annotations": "^2.0",
    "google/apiclient": "^2.18",
    "abraham/twitteroauth": "^7.0",
    "stevenmaguire/oauth2-microsoft": "^2.2",
    "iignatov/lightopenid": "^1.0",
    "jdorn/sql-formatter": "^1.2",
    "joelbutcher/facebook-graph-sdk": "^6.1",
    "laminas/laminas-mvc": "^2.6",
    "laminas/laminas-config": "^2.5",
    "laminas/laminas-http": "^2.5",
    "laminas/laminas-view": "^2.5",
    "laminas/laminas-i18n": "^2.5",
    "laminas/laminas-mail": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "E4u\\": "src/E4u/",
      "E4uTest\\": "tests/src/E4uTest/"
    }
  }
}
